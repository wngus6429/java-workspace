import kr.bit.Book;

public class TPC03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Book b;
		b = new Book();
		b.title = "자바임";
		b.price = 1000;
		b.company = "회사";
		b.page = 700;
		
		System.out.print(b.title + "\t");
		System.out.print(b.price);
		System.out.print(b.company);
		System.out.print(b.page);
	}
}
